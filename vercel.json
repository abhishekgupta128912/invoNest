{
  "version": 2,
  "name": "invonest-frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "frontend/pages/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://invoNest-backend.onrender.com",
    "NEXT_PUBLIC_RECAPTCHA_SITE_KEY": "",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "regions": ["bom1"]
}
